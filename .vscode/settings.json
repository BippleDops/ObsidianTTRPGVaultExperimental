{
     "containers.truncateLongRegistryPaths": true,
     "containers.volumes.groupBy": "VolumeName",
     "markdown.copyFiles.overwriteBehavior": "overwrite",
     "markdown.editor.drop.enabled": "always",
     "markdown.occurrencesHighlight.enabled": true,
     "markdown.preview.breaks": true,
     "markdown.preview.openMarkdownLinks": "inEditor",
     "markdown.preview.typographer": true,
     "markdown.server.log": "debug",
     "markdown.trace.server": "verbose",
     "markdown.updateLinksOnFileMove.enabled": "always",
     "markdown.validate.duplicateLinkDefinitions.enabled": "ignore",
     "markdown.validate.enabled": true,
     "markdown.validate.fileLinks.enabled": "ignore",
     "markdown.validate.unusedLinkDefinitions.enabled": "ignore",
     "markdown.extension.completion.enabled": true,
     "markdown.extension.list.indentationSize": "inherit",
     "markdown.extension.preview.autoShowPreviewToSide": true,
     "markdown.extension.print.imgToBase64": true,
     "markdown.extension.print.onFileSave": true,
     "markdown.extension.tableFormatter.delimiterRowNoPadding": true,
     "markdown.extension.tableFormatter.normalizeIndentation": true,
     "markdown.extension.toc.orderedList": true,
     "material-icon-theme.enableLogging": true,
     "maven.pomfile.autoUpdateEffectivePOM": true,
     "maven.terminal.useJavaHome": true,
     "mediaPreview.video.loop": true,
     "mediaPreview.video.autoPlay": true,
     "vscode-edge-devtools.browserFlavor": "Default",
     "vscode-edge-devtools.webhintInstallNotification": true,
     "database-client.autoSync": true,
     "database-client.disableSQLCompletion": true,
     "database-client.capitalizeKeywordsWhenFormatting": true,
     "database-client.disableTLSVerification": true,
     "npm.enableRunFromFolder": true,
     "npm.runSilent": true,
     "npm-intellisense.packageSubfoldersIntellisense": true,
     "npm-intellisense.scanDevDependencies": true,
     "npm-intellisense.showBuildInLibs": true,
     "path-intellisense.autoSlashAfterDirectory": true,
     "path-intellisense.autoTriggerNextSuggestion": true,
     "php.suggest.basic": true,
     "php.validate.enable": true
